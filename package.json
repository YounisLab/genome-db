{
  "name": "GenomeDB",
  "version": "0.0.1",
  "description": "Genomic data portal.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production --progress",
    "start": "node --es-module-specifier-resolution=node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --es-module-specifier-resolution=node -L src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint src",
    "format": "prettier --config ./.prettierrc.json \"src/**/*.{ts,js,css,scss}\" --write"
  },
  "author": "Fadhil Abubaker",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "antd": "^3.20.0",
    "axios": "^0.18.1",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-import": "^1.11.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^2.0.0",
    "d3": "^5.9.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "express": "^4.16.3",
    "file-loader": "^3.0.0",
    "highcharts": "^9.0.0",
    "highcharts-react-official": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^2.7.2",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.19",
    "mathjs": "^5.4.2",
    "mongodb": "^3.5.7",
    "nodemon": "^1.19.4",
    "react": "^16.5.2",
    "react-csv": "^1.1.1",
    "react-dom": "^16.5.2",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": "13.x"
  },
  "heroku-run-build-script": true,
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.0.2"
  }
}
